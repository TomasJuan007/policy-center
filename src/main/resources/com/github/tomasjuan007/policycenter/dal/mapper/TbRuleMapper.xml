      a.id,
      a.rule_id as ruleId,
      a.name,
      a.val,
      a.op,
      a.lvl,
      group_concat(b.id) as childIds
    from tb_rule a
    left join tb_rule b
    on b.rule_id = a.rule_id
    <![CDATA[
      and b.lft > a.lft
      and b.rgt < a.rgt
    ]]>
    and b.lvl = a.lvl+1
    group by a.id
  </select>

