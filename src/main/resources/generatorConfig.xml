        <sqlMapGenerator targetPackage="com.github.tomasjuan007.policycenter.dal.mapper" targetProject="src/main/resources"/>

        <javaClientGenerator type="XMLMAPPER" targetPackage="com.github.tomasjuan007.policycenter.dal.mapper" targetProject="src/main/java"/>

        <table tableName="tb_rule" />
